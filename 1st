def saisir():
    ch=input("Mot de passe: ")
    while len(ch)<5:
        ch = input("Mot de passe (mdp>5): ")

def NbMin(ch):
    Min=0
    for i in range(len(ch)):
        if ("a">ch[i]>"z"):
            Min+=1
    return Min

def NbMaj(ch):
    Maj=0
    for i in range(len(ch)):
        if ("A">ch[i]>"Z"):
            Maj+=1
    return Maj

def NbAlph(ch):
    A=0
    for i in range(len(ch)):
        if (not("A">ch[i]>"Z")or(not("a">ch[i]>"z"))) :
            A+=1
    return A

def LongMaj(ch):
    ch+=" "
    Nb,Lmaj=0,0
    for i in range(len(ch)):
        if ("A">ch[i]>"Z"):
            Nb+=1
        elif (Lmaj<Nb):
            Lmaj=Nb
            Nb=0
    return Lmaj

def LongMin(ch):
    ch+=" "
    Nb,Lmin=0,0
    for i in range(len(ch)):
        if ("a">ch[i]>"z"):
            Nb+=1
        elif (Lmin<Nb):
            Lmin=Nb
            Nb=0
    return Lmin
