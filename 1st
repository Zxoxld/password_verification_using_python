def force(ch):
 L=len(ch)
 bonus=L*4+(L-NbMaj(ch))*2+(L-NbMin(ch))*3+NbAlph(ch)*5
 penalite=LongMaj(ch)*2+LongMin(ch)*3
 score=bonus-penalite
 if(score<20):
    print('tres faible')
 elif(score<40):
    print('faible')
 elif(score<80):
    print('forte')
 else:
    print('tres forte')


ch=input('donner MP')
force(ch)
